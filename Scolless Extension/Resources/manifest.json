{
    "manifest_version": 2,
    "default_locale": "en",

    "name": "Scrolless",
    "description": "Makes paging down scroll less",
    "version": "1.0",

    "icons": {
        "48": "images/icon-48.png",
        "96": "images/icon-96.png"
    },
    
    "content_scripts": [{
        "js": [ "content.js" ],
        "matches": [
            "*://www.sfchronicle.com/*",
            "*://www.nytimes.com/*",
            "*://theathletic.com/*"
        ]
    }],

    "permissions": [ ]
}
